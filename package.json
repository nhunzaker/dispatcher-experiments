{
  "name": "fluxed",
  "version": "0.0.0",
  "description": "Fluxy stuff",
  "main": "browser.js",
  "scripts": {
    "start": "nodemon server",
    "watch": "watchify . -v -o assets/bundle.js",
    "build": "NODE_ENV=production browserify . | uglifyjs -mc -o assets/bundle.min.js && gzip -9 assets/bundle.min.js -c > assets/bundle.min.js.gz",
    "test": "jest"
  },
  "keywords": [
    "flux"
  ],
  "browserify": {
    "transform": [
      "envify",
      "reactify"
    ]
  },
  "author": "Nate Hunzaker",
  "license": "MIT",
  "dependencies": {
    "collections": "^1.1.0",
    "colors": "^0.6.2",
    "ejs": "^1.0.0",
    "envify": "^1.2.1",
    "errorhandler": "^1.1.0",
    "es6-promise": "^1.0.0",
    "express": "^4.4.3",
    "feedparser": "^0.17.0",
    "node-jsx": "^0.10.0",
    "react": "^0.10.0",
    "reactify": "^0.13.1",
    "route-recognizer": "^0.1.0",
    "rsvp": "^3.0.9",
    "superagent": "^0.18.0"
  },
  "devDependencies": {
    "watchify": "^0.10.2"
  }
}
